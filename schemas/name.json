{
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "name"
    },
    "name": {
      "description": "list of names for the artifact",
      "type": "array",
      "minItems": 1,
      "maxItems": 4,
      "items": {
        "type": "string",
        "pattern": "^[0-9a-z]+$",
        "minLength": 1,
        "maxLength": 32
      }
    },
    "hash": {
      "description": "hash function identifier",
      "type": "string",
      "const": "SHA-256"
    },
    "digest": {
      "description": "digest of the named artifact",
      "type": "string",
      "pattern": "^[0-9a-f]{64}$"
    }
  },
  "required": [
    "type",
    "name",
    "hash",
    "digest"
  ],
  "additionalProperties": false
}
